    <theme id="hideme-20220510132912" name="Référentiel" collapsed="true" icon="fas fa-layer-group"> 
    <!-- Couche permettant l'affichage des noms des communes et de l'affichage du contour rouge sur la commune 
    lors du survol de la souris (hover) de l'utilisateur ainsi que la recherche des noms des communes dans la barre de recherche. -->
            <layer 
            id="communes"
            name="Communes du Parc"
            type="customlayer"     
            url="apps/commun/customlayers/communes.js"      
            visible="true"
            tooltip="true"
            fields="nom"
            aliases="Nom commune"
            tooltipenabled="true"
            infoformat="application/vnd.ogc.gml"
            queryable="false"
            exclusive="false"
            showintoc="true"
            searchable="true"
            searchengine="fuse"
            fusesearchkeys="nom"
            fusesearchresult="{{nom}}">            
        </layer> 
        <!-- tooltip, fields, aliases, tooltipenabled et tooltipcontent permettent le hover, showintoc permet de ne pas afficher la couche dans la légende,
        searchable, searchengine, fusesearchkeys et fusesearchresult permettent la recherche des entités dans la barre de recherche. Name est affiché dans
        la barre de recherche lors de l'affichage de résultats. -->
        <!-- Couche permettant l'affichage du périmètre du Parc -->
