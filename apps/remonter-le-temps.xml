<application    
    title="Remonter le temps sur le territoire du Parc et Géoparc Normandie-Maine"    
    logo=""    
    help="apps/remonter-le-temps/doc_remonter_le_temps.html" 
    style="css/themes/pnrnm.css"    
    exportpng="false"    
    showhelp="false"       
    measuretools="false"
    fullscreen="true"> <!-- Fullscreen indique si la case "Plein écran" a été cochée lors de la sauvegarde du XML -->
</application>
<mapoptions maxzoom="20" projection="EPSG:3857" center="-16656.4,6200926.3" zoom="17" maxextent="-149630.63669321573,6084333.249142119,129823.13871738865,6298356.928340613"/>
<extensions><extension type="component" id="fullscreen" path="apps/commun/addons"/></extensions> <!-- Extension permettant l'ajout du bouton "Plein écran" -->
<olscompletion type="ban" url="https://api-adresse.data.gouv.fr/search/" attribution="La recherche d'adresse est un service proposé par la BAN" />
<searchparameters bbox="false" localities="true" features="true" static="false" animate="true"/>
<baselayers style="gallery"> 
    <baselayer visible="false" id="ortho_ign" thumbgallery="img/basemap/ortho_2022.jpg" title="IGN" label="Fond de carte Photographies aériennes IGN" type="WMTS" url="https://wxs.ign.fr/choisirgeoportail/geoportail/wmts?" layers="ORTHOIMAGERY.ORTHOPHOTOS" format="image/jpeg" fromcapacity="false" attribution="Photographies aériennes : &lt;a href='https://geoservices.ign.fr/services-web-experts' target='_blank'&gt;&lt;img src='img/logo/geoservices.png'&gt;&lt;/a&gt;" style="normal" matrixset="PM" maxzoom="22"  ></baselayer> 
</baselayers> <!-- Un fond de carte est pour cette carte pas nécessaire. Cependant il est impératif de donner un fond de carte. La visibilité est mise sur "false" afin de ne pas afficher le changeur de fond de carte. -->
<themes mini="true" legendmini="false">
        <!-- Thème non affiché dans le panneau de couches grâce à son ID qui commence par "hideme" -->
    <theme id="hideme-20220810114905" name="Balayage" collapsed="true" icon="fas fa-leaf">    
    <!-- Couche nécessaire pour le balayage. Cette couche est cachée afin que l'utilisateur ne puisse pas désactiver la fonctionnalité de balayage. 
    La couche n'est également pas affichée dans la légende afin d'éviter un doublon avec la couche swipeRight. -->
        <layer 
            id="swipeLeft" 
            name="Photographie aérienne de gauche" 
            visible="true" 
            type="customlayer" 
            expanded="true" 
            showintoc="true"
            tiled="true"   
            legendurl="img/blank.gif" 
            url="apps/remonter-le-temps/customcontrols/customlayers/swipeLeft.js" 
            queryable="false"> 
        </layer> <!-- URL indique le fichier dans lequel les couches WMS sont ajoutées pour le côté gauche du balayage. -->
    <!-- Couche permettant d'afficher les limites communales et les noms des communes. -->
        <layer 
            id="communes_parc"
            name="Communes du Parc"
            type="wms"
            url="https://observatoire.parc-naturel-normandie-maine.fr/geoserver/carteetdonnees/wms?"
            visible="true"
            infoformat="text/html"
            queryable="false"
            toplayer="true">
        </layer> 
    <!-- Couche permettant la recherche des noms des communes dans la barre de recherche. -->
        <layer 
            id="communes"
            name="Communes du Parc"
            type="customlayer"     
            url="apps/commun/customlayers/communes.js"      
            visible="true"
            fields="nom"
            aliases="Nom commune"
            infoformat="application/vnd.ogc.gml"
            queryable="false"
            exclusive="false"
            showintoc="true"
            searchable="true"
            searchengine="fuse"
            fusesearchkeys="nom"
            fusesearchresult="{{nom}}">            
        </layer> 
        <!-- Couche affichant le périmètre du Parc en premier plan. -->
        <layer 
            id="perimetreparc"
            name="Périmètre du Parc"
            type="wms"
            url="https://observatoire.parc-naturel-normandie-maine.fr/geoserver/carteetdonnees/ows?"
            visible="true"
            style="perimetre"
            infoformat="text/html"
            queryable="false"
            toplayer="true"><!-- Toplayer précise si la couche demeure figée. Booléen. Défaut = true. Si plusieurs couches sont en toplayer,
elles seront affichées dans l’ordre d’écriture du XML -->
        </layer> 
    </theme> 
    <!-- Thème affiché permettant la sélection des couches et la fonctionnalité de balayage. -->
    <theme id="theme-20171106150925" name="Vue du ciel" collapsed="true" icon="fas fa-layer-group">
        <layer 
            id="swipeRight" 
            name="Comparer les années..." 
            visible="true" 
            type="customlayer" 
            tiled="true"   
            legendurl="img/blank.gif" 
            url="apps/remonter-le-temps/customcontrols/customlayers/swipeRight.js" 
            customcontrol="true" 
            customcontrolpath="apps/remonter-le-temps/customcontrols/customcontrols/" 
            queryable="false"> 
        </layer> <!-- URL indique le fichier dans lequel les couches WMS sont ajoutées pour le côté droit du balayage.
                      CUSTOMCONTROLPATH indique le chemin d'accès vers les fichiers permettant de personnaliser le bouton et la barre de balayage
                      et la légende avec les listes déroulantes. -->
    </theme>
</themes>
</config>