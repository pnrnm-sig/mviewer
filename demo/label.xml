<?xml version="1.0" encoding="UTF-8"?>
<config>
  <application id="label" lang="fr" langfile="demo/demo.i18n.json"
    title="Démo de l'addon label" showhelp="false" logo="" style="css/themes/default.css"
    exportpng="false" geoloc="false" measuretools="false" coordinates="false" mouseposition="false"
    togglealllayersfromtheme="true" />

  <mapoptions maxzoom="20" projection="EPSG:3857" rotation="true" center="285105.56,6227608.18"
    zoom="9"
    projextent="-20037508.342789244, -20037508.342789244, 20037508.342789244, 20037508.342789244" />

  <olscompletion type="ban" url="https://api-adresse.data.gouv.fr/search/"
    attribution="La recherche d'adresse est un service proposé par l'API adresse.data.gouv.fr" />

  <searchparameters localities="true" features="false" bbox="false" querymaponclick="true"
    closeafterclick="false"
    banmarker="false"
    animate="false" duration="800"
    imgurl='' imgwidth='' svgcolor='red' />

  <baselayers style="gallery">
    <baselayer visible="false" type="OSM" id="osm1" label="OpenStreetMap" title="OpenStreetMap"
      thumbgallery="img/basemap/osm.png" url="http://{a-c}.tile.openstreetmap.org/{z}/{x}/{y}.png" />
    <baselayer visible="false" type="OSM" id="darkmatter" label="Dark Matter" title="CartoDb"
      thumbgallery="img/basemap/darkmatter.png"
      url="http://{a-c}.basemaps.cartocdn.com/dark_all/{z}/{x}/{y}.png" maxzoom="20"
      attribution="Map tiles by CartoDb, under CC BY 3.0" />
    <baselayer visible="true" id="positron" thumbgallery="img/basemap/positron.png" title="CartoDb"
      label="Positron"
      type="OSM" url="https://{a-c}.basemaps.cartocdn.com/light_all/{z}/{x}/{y}.png"
      attribution="Map tiles by  &lt;a href=&quot;https://cartodb.com/attributions&quot;&gt;CartoDb&lt;/a&gt;, under  &lt;a href=&quot;https://creativecommons.org/licenses/by/3.0/&quot;&gt;CC BY 3.0 &lt;/a&gt;" />
    <baselayer visible="false" id="esriworldimagery" thumbgallery="img/basemap/esriworldwide.jpg"
      title="Esri world imagery"
      label="Esri world imagery" type="OSM"
      url="http://server.arcgisonline.com/ArcGIS/rest/services/World_Imagery/MapServer/tile/{z}/{y}/{x}"
      attribution="Esri world imagery"></baselayer>
  </baselayers>

  <extensions>
    <extension type="component" id="label" path="apps/addons" />
  </extensions>

  <themes mini="true" legendmini="false">

    <theme id="theme_label" name="Baignade" collapsed="true" icon="fas fa-tint">

      <layer
        id="baignade"
        name="Sites de baignade"
        type="customlayer"
        customcontrol="true"
        customcontrolpath="demo/label/customcontrols"
        infoformat="text/html"
        expanded="true"
        url="demo/label/customlayers/baignade.js"
        visible="true"
        index="1"
        infohighlight="true"
        legendurl="img/blank.gif"
        vectorlegend="true"
        queryable="false"
        showintoc="true">
      </layer>

      <layer
        id="ars:admin_dpt_idf"
        name="Contours départements Ile-de-France"
        type="wms"
        url="https://santegraphie.fr/geoserver/ows"
        infoformat="text/html"
        queryable="false"
        index="3"
        showintoc="false"
        visible="true">
      </layer>

    </theme>

  </themes>

</config>